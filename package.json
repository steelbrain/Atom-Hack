{
  "name": "Atom-Hack",
  "main": "./lib/index.js",
  "version": "7.0.0",
  "description": "HackLang support for Atom Editor",
  "author": "steelbrain",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "(flow check) && (eslint . )"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1",
    "node-ssh": "^3.0.1",
    "sb-exec": "^3.0.2",
    "sb-promisify": "^2.0.1",
    "shell-escape": "^0.2.0"
  },
  "package-deps": [
    "linter",
    "language-hack"
  ],
  "repository": "https://github.com/steelbrain/atom-hack",
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    },
    "atom-hack": {
      "versions": {
        "1.0.0": "provideDelegateRegistry"
      }
    }
  },
  "devDependencies": {
    "eslint-config-steelbrain": "^1.0.4",
    "flow-bin": "^0.42.0"
  },
  "configSchema": {
    "supportBashOnWindows": {
      "title": "Enable support for Bash on Windows",
      "type": "boolean",
      "default": true
    }
  }
}
